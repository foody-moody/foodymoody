{
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "es5",
  "useTabs": false,
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "endOfLine": "auto",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^utils/(.*)$",
    "^apis/(.*)$",
    "^hooks/(.*)$",
    "^recoils/(.*)$",
    "^pages/(.*)$",
    "^common/(.*)$",
    "^components/(.*)$",
    "^[./]",
    "^constants/(.*)$"
  ]
}
